<!DOCTYPE html>

<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet"/>
</head>
<body>
<div class="m-2">
    <img src="/images/blog-top.jpg" asp-append-version="true" class="m-2" />
    <h6 class="bg-primary text-white m-2 p-2">Uncached timestamp: @DateTime.Now.ToLongTimeString()</h6>
    <cache expires-after="@TimeSpan.FromSeconds(10)">
        <h6 class="bg-primary text-white m-2 p-2">Cached timestamp: @DateTime.Now.ToLongTimeString()</h6>
    </cache>
    @RenderBody()
</div>
</body>
</html>
